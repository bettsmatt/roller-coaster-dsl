grammar org.xtext.rollercoaster.dsl.Coaster with org.eclipse.xtext.common.Terminals

generate coaster "http://www.xtext.org/rollercoaster/dsl/Coaster"

Model:
	rollerCoaster += RollerCoaster;
	
	
	
RollerCoaster:
	name=ID
	track += Track*
	cart += Cart*;	

	
Track:
	'Track' name=ID length=INT (elevationChange=INT)? (angle=SignedInt)? ;

Cart:
	'Cart' name=ID;
	
SignedInt:
	sign=('-')? value=INT;
